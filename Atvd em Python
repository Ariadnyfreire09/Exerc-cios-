#54#
x = 1
while x < 50:
  print(x)
  x = x + 2


#74#
print("TABELA DO PREÇO DE PÃES")

Q = 1
while Q <= 50:
  print(Q, " - ", 0.18 * Q)
  Q = Q + 1
